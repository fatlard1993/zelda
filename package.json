{
	"name": "zelda",
	"version": "2.7.4",
	"description": "Local development package link manager.",
	"main": "src/index.js",
	"bin": "src/index.js",
	"scripts": {
		"start": "bun run src/index.js",
		"lint": "bun --bun eslint",
		"lint:fix": "bun --bun eslint --fix",
		"format": "bun run lint:fix && bun --bun prettier --write . | grep -q unchanged",
		"version": "npm version"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fatlard1993/zelda.git"
	},
	"author": "feross, fatlard1993",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/fatlard1993/zelda/issues"
	},
	"homepage": "https://github.com/fatlard1993/zelda#readme",
	"dependencies": {
		"argi": "github:fatlard1993/argi#fcac703",
		"find-root": "^1.1.0",
		"log": "github:fatlard1993/log#e4cb364"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.23.0",
		"bun-types": "^1.2.8",
		"eslint": "^9.23.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsdoc": "^50.6.9",
		"eslint-plugin-spellcheck": "0.0.20",
		"eslint-plugin-testing-library": "^7.1.1",
		"eslint-plugin-unicorn": "^58.0.0",
		"eslint-plugin-write-good-comments": "^0.2.0",
		"globals": "^16.0.0",
		"prettier": "^3.5.3"
	}
}
